{
  "name": "@qualithm/npm-example",
  "version": "0.1.0",
  "description": "Production-ready template for NPM packages.",
  "private": false,
  "type": "module",
  "keywords": [
    "typescript",
    "template",
    "npm"
  ],
  "license": "MIT",
  "author": "Qualithm",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qualithm/npm-example.git"
  },
  "homepage": "https://github.com/qualithm/npm-example#readme",
  "bugs": {
    "url": "https://github.com/qualithm/npm-example/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "bun": "./dist/index.js",
      "node": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "bench": "bun run bench/index.ts",
    "build:watch": "tsc -p tsconfig.build.json --watch",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "demo": "bun run scripts/demo.ts",
    "dev": "bun --watch run src/index.ts",
    "docs": "typedoc",
    "format:fix": "prettier --write .",
    "format": "prettier --check .",
    "lint:fix": "eslint --fix .",
    "lint": "eslint .",
    "prepublishOnly": "bun run clean && bun run build",
    "start": "bun run src/index.ts",
    "test:coverage": "bun test --coverage",
    "test:unit": "bun test src/__tests__/unit",
    "test:watch": "bun test --watch",
    "test": "bun test",
    "typecheck": "tsc -p tsconfig.node.json --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "10.0.1",
    "@types/bun": "1.3.9",
    "eslint": "10.0.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "4.4.1",
    "globals": "17.3.0",
    "jiti": "2.6.1",
    "prettier": "3.8.1",
    "typedoc": "0.28.17",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.0"
  },
  "overrides": {
    "minimatch": "10.2.1"
  }
}
